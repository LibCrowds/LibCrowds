{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 9"]
      }
    }],
    "stage-2",
    "backpack-core/babel"
  ],
  "plugins": ["transform-runtime"],
  "env": {
    "testing": {
      "presets": [
        "env",
        "stage-2",
        "backpack-core/babel"
      ],
      "plugins": [
        [
          "module-resolver",
          {
            "root": ["."],
            "alias": {
              "@": "."
            }
          }
        ]
      ]
    }
  }
}
