{
  "name": "libcrowds",
  "version": "1.0.0-beta.4",
  "description": "A Vue.js frontend for PYBOSSA.",
  "author": "Alex Mendes <alexanderhmendes@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "backpack dev",
    "build": "backpack build && nuxt build",
    "start": "nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "unit": "cross-env NODE_ENV=testing jest",
    "test": "npm run lint && npm run unit"
  },
  "dependencies": {
    "@nuxtjs/google-analytics": "^1.0.0",
    "@nuxtjs/pwa": "^1.0.2",
    "axios": "^0.16.2",
    "bootstrap": "4.0.0-beta",
    "bootstrap-vue": "^1.0.0-beta.9",
    "capitalize": "^1.0.0",
    "chartist-plugin-legend": "^0.6.2",
    "chartist-plugin-tooltips": "0.0.17",
    "cookieconsent": "^3.0.4",
    "croppie": "^2.5.0",
    "cross-env": "^5.0.1",
    "errorhandler": "^1.5.0",
    "express": "~4.15.3",
    "express-session": "^1.15.6",
    "file-saver": "^1.3.3",
    "firebase": "^4.5.2",
    "hamburgers": "^0.9.1",
    "libcrowds-viewer": "^3.3.1",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "moment": "^2.19.1",
    "nuxt": "1.0.0-rc11",
    "popper.js": "^1.12.5",
    "source-map-support": "^0.4.15",
    "spinkit": "^1.2.5",
    "vue-awesome": "^2.3.3",
    "vue-chartist": "^2.1.1",
    "vue-clickaway": "^2.1.0",
    "vue-form-generator": "^2.0.0",
    "vue-gravatar": "^1.1.3",
    "vue-js-toggle-button": "^1.2.0",
    "vue-notification": "^1.3.4",
    "vue-progress": "^0.2.1",
    "vue-scrollto": "^2.7.8",
    "vue-sweetalert": "^0.1.18"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "babel-runtime": "^6.26.0",
    "backpack-core": "^0.4.3",
    "chalk": "^2.1.0",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "html-loader": "^0.5.1",
    "jest": "^21.2.1",
    "jest-serializer-html": "^4.0.0",
    "jest-vue-preprocessor": "~1.2.0",
    "jsdom": "^11.3.0",
    "markdown-loader": "^2.0.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.7.8",
    "svg-inline-loader": "^0.8.0",
    "vue-server-renderer": "^2.4.4",
    "vue-template-es2015-compiler": "^1.5.3",
    "vue-test-utils": "^1.0.0-beta.2",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/test/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/test/assetsTransformer.js",
      "(.*)/local.config$": "<rootDir>/test/test.local.config.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!vue-awesome)"
    ],
    "collectCoverageFrom": [
      "components/**/*.vue",
      "layouts/**/*.vue",
      "middleware/**/*.js",
      "modules/**/*.js",
      "pages/**/*.vue",
      "store/**/*.js",
      "utils/**/*.js"
    ],
    "coverageDirectory": "<rootDir>/test/unit/coverage",
    "testRegex": "/test/unit/specs/.*(test|spec)\\.jsx?$",
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  }
}
