    "vue-awesome": "^2.3.1",
    "vue-chartist": "^2.1.0",
    "vue-cookie": "^1.1.4",
    "vue-form-generator": "^2.0.0",
    "vue-gravatar": "^1.1.3",
    "vue-images": "^1.0.10",
    "vue-js-toggle-button": "^1.1.2",
    "vue-meta": "^1.0.5",
    "vue-progressbar": "^0.7.2",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
