{
  "name": "libcrowds",
  "version": "1.0.0-beta.8",
  "description": "A Vue.js frontend for PYBOSSA.",
  "author": "Alex Mendes <alexanderhmendes@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "optimise": "node bin/convertToWebp.js",
    "dev": "backpack dev",
    "build": "backpack build && nuxt build",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "unit": "cross-env NODE_ENV=testing jest --coverage",
    "unit:update": "cross-env NODE_ENV=testing jest -u",
    "test": "npm run lint && npm run unit"
  },
  "dependencies": {
    "@nuxtjs/axios": "^4.4.0",
    "@nuxtjs/google-analytics": "^2.0.1",
    "@nuxtjs/proxy": "^1.1.2",
    "@nuxtjs/pwa": "^2.0.2",
    "bootstrap": "4.0.0-beta.2",
    "bootstrap-vue": "^2.0.0-rc.0",
    "capitalize": "^1.0.0",
    "chartist-plugin-tooltips": "0.0.17",
    "clipboard": "^1.7.1",
    "cookie": "^0.3.1",
    "cookieconsent": "^3.0.4",
    "croppie": "^2.5.0",
    "cross-env": "^5.0.1",
    "es6-promise": "^4.1.1",
    "express": "~4.16.2",
    "file-saver": "^1.3.3",
    "git-rev-sync": "^1.9.1",
    "http-status": "^1.0.1",
    "izitoast": "^1.1.5",
    "js-cookie": "^2.2.0",
    "libcrowds-viewer": "^4.0.1",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "nuxt": "1.0.0-rc11",
    "progressbar.js": "^1.0.1",
    "query-string": "^5.0.1",
    "raven": "^2.2.1",
    "raven-js": "^3.19.1",
    "source-map-support": "^0.5.0",
    "sweetalert2": "^7.0.5",
    "velocity-animate": "^1.5.0",
    "vue-awesome": "^2.3.3",
    "vue-chartist": "^2.1.1",
    "vue-clickaway": "^2.1.0",
    "vue-confetti": "^0.3.3",
    "vue-form-generator": "^2.1.1",
    "vue-gravatar": "^1.1.3",
    "vue-infinite-loading": "^2.2.1",
    "vue-js-toggle-button": "^1.2.0",
    "vue-moment": "^3.0.0",
    "vue-multiselect": "^2.0.3",
    "vue-notifications": "^0.9.0",
    "vue-prevent-parent-scroll": "^1.0.5",
    "vue-progress": "^0.2.1",
    "vue-scrollto": "^2.7.8",
    "vue-simplemde": "^0.4.6",
    "vue-sweetalert": "^0.1.18"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "babel-runtime": "^6.26.0",
    "backpack-core": "^0.4.3",
    "chalk": "^2.1.0",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-babel-module": "^4.0.0-beta.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "html-loader": "^0.5.1",
    "imagemin": "^5.3.1",
    "imagemin-webp": "^4.0.0",
    "jest": "^21.2.1",
    "jest-serializer-html": "^4.0.0",
    "jest-vue-preprocessor": "^1.3.1",
    "jsdom": "^11.3.0",
    "json-loader": "^0.5.7",
    "markdown-loader": "^2.0.1",
    "modernizr": "^3.5.0",
    "modernizr-loader": "^1.0.1",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.0.2",
    "postcss-url": "^7.1.2",
    "regenerator-runtime": "^0.11.0",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.7.8",
    "vue-template-es2015-compiler": "^1.6.0",
    "vue-test-utils": "1.0.0-beta.7",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/test/assetsTransformer.js",
      "\\.(scss)$": "<rootDir>/test/assetsTransformer.js",
      "(.*)/local.config$": "<rootDir>/test/test.local.config.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!vue-awesome)"
    ],
    "collectCoverageFrom": [
      "components/**/*.vue",
      "layouts/**/*.vue",
      "middleware/**/*.js",
      "modules/**/*.js",
      "pages/**/*.vue",
      "store/**/*.js",
      "utils/**/*.js"
    ],
    "coverageDirectory": "<rootDir>/test/unit/coverage",
    "testRegex": "/test/unit/specs/.*(test|spec)\\.jsx?$",
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  }
}
