    "inject-loader": "^3.0.1",
    "jest": "^20.0.4",
    "jest-vue-preprocessor": "^1.1.0",
    "lolex": "^1.5.2",
    "markdown-loader": "^2.0.1",
    "nightwatch": "^0.9.16",
